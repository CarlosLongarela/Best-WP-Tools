{% set page_title = "WPO (Tab 2)" -%}
{% set section = "wpo" -%}

{% include "partials/header.njk" %}

			<h1>{% raw %}{{msg_medition_tools}}{% endraw %}</h1>

			<div id="menu_wpo_tools">
				<div id="section_wpo" class="item_wpo active_tab">WPO</div>
				<div id="section_security" class="item_wpo">{% raw %}{{msg_security}}{% endraw %}</div>
				<div id="section_ttfb" class="item_wpo">TTFB</div>
				<div id="section_http2" class="item_wpo">HTTP/2-3</div>
				<div id="section_dns" class="item_wpo">DNS</div>
				<div id="section_gzip" class="item_wpo">GZIP</div>
				<div id="section_mail" class="item_wpo">MAIL</div>
			</div>

			<div class="wpo_tab visible" id="target_section_wpo">
				<h2>WPO</h2>

				<input id="pagespeed_insights" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_pagespeed_insights}}{% endraw %}">

				<p class="txt_notice">* {% raw %}{{msg_other_wpo_links}}{% endraw %}</p>

				<div class="cl-flex-50">
					<a class="btn btn_50 btn_bold_border" href="https://tools.pingdom.com/" target="_blank" rel="noopener noreferrer">Pingdom Website Speed Test</a>
					<a class="btn btn_50 btn_bold_border" href="https://gtmetrix.com/" target="_blank" rel="noopener noreferrer">GTMetrix</a>
					<a class="btn btn_50 btn_bold_border" href="https://www.webpagetest.org/" target="_blank" rel="noopener noreferrer">WebPageTest</a>
					<a class="btn btn_50 btn_bold_border" href="https://tools.keycdn.com/speed" target="_blank" rel="noopener noreferrer">keycdn Website Speed Test</a>
				</div>
			</div>

			<div class="wpo_tab" id="target_section_security">
				<h2>{% raw %}{{msg_security}}{% endraw %}</h2>
				<input id="sucuri_sitecheck" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_sucuri_sitecheck}}{% endraw %}">
			</div>

			<div class="wpo_tab" id="target_section_ttfb">
				<h2>TTFB (Time To First Byte)</h2>
				<input id="sucuri_loadtimetester" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_sucuri_loadtimetester}}{% endraw %}">
				<input id="byte_check" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_byte_check}}{% endraw %}">
			</div>

			<div class="wpo_tab" id="target_section_http2">
				<h2>HTTP/2-3</h2>
				<input id="http2_pro" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_http2_pro}}{% endraw %}">
				<br />
				<h2>HTTP/3</h2>
				<input id="http3_pro" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_http3_pro}}{% endraw %}">
			</div>

			<div class="wpo_tab" id="target_section_dns">
				<h2>DNS</h2>
				<input id="mx_toolbox" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_mx_toolbox}}{% endraw %}">
				<input id="dns_checker" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_dns_checker}}{% endraw %}">
				<input id="dns_checker_ipv6" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_dns_checker_ipv6}}{% endraw %}">
				<input id="dns_historical" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_dns_historical}}{% endraw %}">
			</div>

			<div class="wpo_tab" id="target_section_gzip">
				<h2>Gzip</h2>
				<form id="url" action="https://www.giftofspeed.com/gzip-test/" method="post" target="_blank">
					<input id="check_gzip_url_value" type="hidden" name="url" value="" >
					<input id="check_gzip_compression" type="submit" class="btn btn_green full_width" value="{% raw %}{{btn_check_gzip_compression}}{% endraw %}">
				</form>
			</div>

			<div class="wpo_tab" id="target_section_mail">
				<h2>Mail</h2>
				<input id="mx_mx_toolbox" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_mx_mx_toolbox}}{% endraw %}">
				<input id="smtp_mx_toolbox" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_smtp_mx_toolbox}}{% endraw %}">
				<input id="blacklist_mx_toolbox" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_blacklist_mx_toolbox}}{% endraw %}">
				<input id="mx_gsuite" type="button" class="btn btn_green full_width" value="{% raw %}{{btn_gsuite}}{% endraw %}">
			</div>

{% include "partials/footer.njk" -%}
